{
  "name": "flux-obsidian-plugin",
  "version": "0.2.1",
  "description": "Flux sync plugin for Obsidian - syncs vault to Flux server",
  "main": "main.js",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "vault": "pnpm build && node scripts/deploy.mjs",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "obsidian": "latest",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
